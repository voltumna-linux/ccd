[Unit]
Description=RNM (DPDK) service
Requires=local-fs.target rnm-dpdk-conf.service
After=rnm-dpdk-conf.service

[Service]
EnvironmentFile=/run/rnm-dpdk/rnm.conf
ExecStartPre=/usr/bin/dpdk-devbind.py --bind=vfio-pci $RNM_NETIF
ExecStart=/usr/sbin/rnm-dpdk -l 1 -n 1 -m 128 --file-prefix rnm -a $RNM_NETIF $CLIENTID

[Install]
WantedBy=multi-user.target
